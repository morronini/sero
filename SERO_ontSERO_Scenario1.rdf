<owl:Class rdf:about="#ChemicalDamageCorrosion">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#ChemicalDamage"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasCausation"/>
          <owl:hasValue rdf:resource="#CorrosiveExposure"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#ChemicalDamage"/>
</owl:Class>

