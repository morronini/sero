<owl:Class rdf:about="#Profile">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#MaterialObjectStructure"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#hasProfilePart"/>
          <owl:someValuesFrom rdf:resource="#ProfilePart"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
